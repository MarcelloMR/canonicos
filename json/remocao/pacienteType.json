{
    "$schema": "http://json-schema.org/schema#", 
    "$id": "remocao/pacienteType.json",   
    "title": "paciente",
    "description": "Paciente transportado na remoção",
    "version": "1.0",
    "type": "object",
    "properties": {
        "nome":{
            "type":"string"
        },
        "idade":{
            "type":"number"
        },
        "frequenciaCardiaca":{
            "type":"number"
        },
        "saturacao":{
            "type":"number"
        },
        "sistole":{
            "type":"number"
        },
        "diastole":{
            "type":"number"
        },
        "detalhes":{
            "type":"number"
        },
        "neonato":{
            "type":"boolean"
        }
    },
    "required": ["nome","idade","neonato"]
}